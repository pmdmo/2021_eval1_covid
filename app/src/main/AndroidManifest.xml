<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.iessaladillo.pedrojoya.tipcalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TipCalculator"
        android:fullBackupContent="@xml/backup_descriptor">
        <activity
            android:name="es.iessaladillo.pedrojoya.walkingdead.ui.add_character.AddCharacterActivity"
            android:label="@string/add_character"
            android:parentActivityName="es.iessaladillo.pedrojoya.walkingdead.ui.characters.MainActivity"
            android:exported="false" />
        <activity
            android:name="es.iessaladillo.pedrojoya.walkingdead.ui.characters.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>